{
  "name": "my-grpc-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare:grpc": "mkdir src\\generated 2>nul || exit 0",
    "generate:grpc": "npx protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_opt=grpc-js,outputServices=grpc-js --ts_proto_out=./src/generated --proto_path=../taskrr-service/src/main/proto ../taskrr-service/src/main/proto/*.proto"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "axios": "^1.7.7",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.12.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-proto": "^2.2.3",
    "typescript": "^5"
  }
}
